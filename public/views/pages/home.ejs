<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
    <link rel="stylesheet" href="css/normalize.css">
    <link rel="stylesheet" href="css/home.css">
    <link rel="stylesheet" href="css/detalleProduct.css">
</head>
<body>
<%- include("../partials/menuHeader.ejs")  %>  

    <!-- Seccion del carrusel -->
    <section class="contenedor-carrusel">
        <!-- contenedor de box-imagenes -->
        <div class="contenedor-imagenes-carrusel">
            <!-- imagenes del carrusel -->
            <ul class="slider">
                <li><img src="img/header2.jpg" alt="Images1"></li>
                <li><img src="img/icons/img/TARJETAS/amex@2x.png" alt="images2"></li>
                <li><img src="img/oso.png" alt="images1" alt="images3"></li>
                
            </ul>
        </div>
    </section>
    <div class="separador"></div>
    <!-- seccion de iconos edades y ofertas -->
    <section class="contenido">

        <article class="contenidos">
            <!-- contenedor truck card dollars -->
            <div class="iconos">
                <div class="contenido-iconos">
                    <div class="personalizado">
                        <i class="fas fa-truck"></i>
                    </div>
                    
                    <div class="contenido">
                        <h5>ENVIOS A TODO EL PAIS</h5>
                        <p>costo bajos</p>
                    </div>
                    
                </div>
                <div class="contenido-iconos">
                    <div class="personalizado">
                        <i class="far fa-credit-card"></i>
                    </div>
                    
                    <div class="contenido">
                        <h5>TODOS LOS MEDIOS DE PAGO</h5>
                        <p>Crédito y débito</p>
                    </div>
                </div>
                <div class="contenido-iconos">
                    <div class="personalizado">
                        <i class="fas fa-dollar-sign"></i>
                    </div>
                    
                    <div class="contenido">
                        <h5>20% OFF</h5>
                        <p>Pagando en efectivo</p>
                    </div>
                </div>                
            </div>
        </article>
        <!-- Productos por edades -->
        <article class="contenidos">
            <h2>BUSCÁ POR EDADES</h2>
            <div class="contenedor-imagenes-edades">
                <div class="contenedor-img">
                    <div class="div-Imagenes"><img src="img/icons/img/baby_0_24.jpg" alt="de 0 a 24" class="imagenes-edades"><h4>DE 0 A 24 MESES</h4></div>
                    <div class="div-Imagenes"><img src="img/icons/img/edad_2_3.jpg" alt="de 2 a 3" class="imagenes-edades"><h4>DE 2 A 3 AÑOS</h4></div>
                    <div class="div-Imagenes"><img src="img/icons/img/de_4_6.jpg" alt="de 4 a 6" class="imagenes-edades"><h4>DE 4 A 6 AÑOS</h4></div>  
                </div>
                <!-- <div class="contenedor-titulos">
                    <h2>MÁS POPULARES</h2>
                    
                    
                </div> -->
                
                <div class="contenedor-img">
                    <div class="div-Imagenes">
                        <h2>MÁS POPULARES</h2>
                        <img src="img/icons/img/toys1.jpg" alt="boligrafo Multicolor" class="imagenes-edades"><h4>BOLIGRAFO MULTICOLOR</h4><p>$1300 | 20% OFF</p>
                    </div>
                    <div class="div-Imagenes">
                        <h2>OFERTAS</h2>
                        <img src="img/icons/img/michal-bozek-RcxR1aLw8X0-unsplash.jpg" alt="cubo de madera" class="imagenes-edades">
                        <h4>CUBO DE MADERA</h4>
                        <p>$3900 | 10% OFF</p>
                    </div>
                    <div class="div-Imagenes">
                        <h2>ENVIOS GRATIS</h2>
                        <img src="img/icons/img/trenes.jpg" alt="tren de madera" class="imagenes-edades">
                        <h4>TREN DE MADERA</h4>
                        <p>$3299 | 45% OFF</p>
                    </div>
                    
                
                    
                
                    
                </div>               
            </div>
        </article>
        <!-- <article class="contenidos"></article> -->
    </section>
    <!-- footer -->
<%- include("../partials/footerBase.ejs")  %>
</body>
</html>


















